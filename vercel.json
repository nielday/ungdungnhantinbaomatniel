{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/admin",
      "dest": "/admin"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "/api/admin/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "https://ungdungnhantinbaomatniel-production.up.railway.app/api/$1"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "https://ungdungnhantinbaomatniel-production.up.railway.app/uploads/$1"
    }
  ]
}
